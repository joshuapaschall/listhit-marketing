{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "listhit-marketing.vercel.app" }],
      "destination": "https://listhit.io/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "header", "key": "x-forwarded-proto", "value": "http" }],
      "destination": "https://listhit.io/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/_next/static/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "max-age=0, must-revalidate" }
      ]
    }
  ]
}
